<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#6A6A68" />
            <stroke
                android:width="1dp"
                android:color="#F6F6F4" />
            <corners
                android:radius="10dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>

    </item>

    <item>
        <shape>
            <stroke
                android:color="#F6F6F4"
                android:width="1dp"/>
            <corners
                android:radius="10dp"/>
        </shape>
    </item>



</selector>